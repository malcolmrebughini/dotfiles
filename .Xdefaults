*.font:  Hack:hinting=true:style=Regular:pixelsize=18
rofi.font: Hack 18

!! Appearance
urxvt.termName: rxvt-unicode
urxvt.scrollBar: false
urxvt*depth: 32
!urxvt.background: rgba:0000/0000/0000/2000
!urxvt.foreground: gray
!! Font prefferenes
!urxvt.font: xft:DejaVu Sans Mono:pixelsize=20
!urxvt.boldFont: xft:DejaVu Sans Mono:pixelsize=20:weight=bold
!urxvt.letterSpace: -1
!! Larger history limit
urxvt.saveLines: 1000000
!urxvt*font: xft:Terminus:pixelsize=20,xft:Inconsolata\ for\ Powerline:pixelsize=20
!urxvt*font: xft:Source\ Code\ Pro\ Medium:pixelsize=21:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true,xft:PowerlineSymbols
urxvt.letterSpace: -1
!urxvt.font: xft:Terminess Powerline

urxvt.font: xft:Monospace\ Regular:style=Regular:size=13

URxvt.perl-ext:	  default,matcher
URxvt.url-launcher: chromium
URxvt.matcher.button: 1
